# Models - Use format: provider/model_name
# Providers: openai, anthropic, groq, ollama
# Examples:
#   openai/gpt-4
#   anthropic/claude-3-5-sonnet-20241022
#   groq/llama-3.3-70b-versatile
#   ollama/qwen2.5:7b
PRO_MODEL_1=groq/llama-3.3-70b-versatile
PRO_MODEL_2=ollama/qwen2.5:7b
CON_MODEL_1=groq/llama-3.3-70b-versatile
CON_MODEL_2=ollama/qwen2.5:7b
JUDGE_MODEL=groq/llama-3.3-70b-versatile

# API Keys
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
GROQ_API_KEY=
OLLAMA_BASE_URL=http://127.0.0.1:11434

# Web Search
SEARCH_ENGINE=duckduckgo
MAX_SEARCH_RESULTS=8
MAX_SCRAPED_PAGES_PER_FACTOR=5
SCRAPE_TIMEOUT=15

# Debate
DEBATE_ROUNDS=3
MAX_ARGUMENT_LENGTH=200
ALLOW_CROSS_CRITIQUE=true
MAX_FACTORS=5

# Evaluation
ENABLE_ANONYMIZATION=true
SCORING_SCALE=1-10

# Output
OUTPUT_DIR=outputs/
SAVE_TRANSCRIPTS=true
